{
    "ver": "0.0.1",
    "authors": [
        "swwang"
    ],
    "contact": "",
    "categories": ["active"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameter-groups": [
        {
            "id": "rdp_clsf",
            "parameters": [
                "conf",
                "gene",
                "minWords"
            ],
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "with_border": true
        }
    ],
    "parameters": [ 
        {
            "id": "amplicon_set_upa",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseExperiments.AmpliconSet" ]
            }
        },{
            "id": "output_name",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ null ],
            "field_type": "text",
            "text_options": {
                "valid_ws_type": ["KBaseExperiments.AmpliconSet", "KBaseMatrices.AmpliconMatrix"],
                "is_output_name": true
            }
        },{
            "id": "conf",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "field_type": "text",
            "default_values": [ "0.8" ],
            "text_options": {
                "validate_as": "float",
                "min_float": "0",
                "max_float": "1"
            }
        },{
            "id": "gene",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "16srrna" ],
            "field_type": "dropdown",
            "dropdown_options": {
                "options": [
                    {
                        "value": "16srrna",
                        "display": "16srrna",
                        "id": "16srrna",
                        "ui-name": "RDP 16srrna"
                     },{
                        "value": "fungallsu",
                        "display": "fungallsu",
                        "id": "fungallsu",
                        "ui-name": "RDP fungallsu"
                    },{
                        "value": "fungalits_warcup",
                        "display": "fungalits_warcup",
                        "id": "fungalits_warcup",
                        "ui-name": "RDP fungalits_warcup"
                    },{
                        "value": "fungalits_unite",
                        "display": "fungalits_unite",
                        "id": "fungalits_unite",
                        "ui-name": "RDP fungalits_unite"
                    }
                ]
            }
        },{
            "id": "minWords",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "field_type": "text",
            "default_values": [ null ],
            "text_options": {
                "validate_as": "int",
                "min_int": 5
            }
        },{
            "id": "write_ampset_taxonomy",
            "optional": true,
            "advanced": true,
            "allow-multiple": false,
            "field_type": "dropdown",
            "dropdown_options": {
                "options": [
                    {
                        "value": "do_not_write",
                        "display": "do not write"
                    },{
                        "value": "do_not_overwrite",
                        "display": "do not overwrite"
                    },{
                        "value": "overwrite",
                        "display": "overwrite"
                    }
                ]
            },
            "default_values": ["do_not_overwrite"]
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "kb_RDP_Classifier",
            "method": "run_classify",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },{
                    "narrative_system_variable": "workspace_id",
                    "target_property": "workspace_id"
                },{
                    "input_parameter": "amplicon_set_upa",
                    "target_property": "amplicon_set_upa",
                    "target_type_transform": "resolved-ref"
                },{
                    "input_parameter": "output_name",
                    "target_property": "output_name"
                },{
                    "input_parameter": "rdp_clsf",
                    "target_property": "rdp_clsf"
                },{
                    "input_parameter": "write_ampset_taxonomy",
                    "target_property": "write_ampset_taxonomy"
                }
            ],
            "output_mapping": [
                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },{
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
