{
    "ver": "0.0.1",
    "authors": [
        "swwang"
    ],
    "contact": "",
    "categories": ["active"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameter-groups": [
        {
            "id": "rdp_clsf",
            "parameters": [
                "conf",
                "gene",
                "minWords"
            ],
            "optional": false,
            "advanced": true,
            "allow_multiple": false,
            "with_border": true
        }
    ],
    "parameters": [ 
        {
            "id": "amp_mat_upa",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [ "KBaseMatrices.AmpliconMatrix" ]
            }
        },{
            "id": "output_name",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_type": ["KBaseMatrices.AmpliconMatrix"],
                "is_output_name": true
            }
        },{
            "id": "conf",
            "optional": false,
            "advanced": true,
            "allow_multiple": false,
            "field_type": "text",
            "default_values": [ "0.8" ],
            "text_options": {
                "validate_as": "float",
                "min_float": "0",
                "max_float": "1"
            }
        },{
            "id": "gene",
            "optional": false,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [ "silva_138_ssu" ],
            "field_type": "dropdown",
            "dropdown_options": {
                "options": [
                    {
                        "value": "16srrna",
                        "display": "16srrna"
                     },{
                        "value": "fungallsu",
                        "display": "fungallsu"
                    },{
                        "value": "fungalits_warcup",
                        "display": "fungalits_warcup"
                    },{
                        "value": "fungalits_unite",
                        "display": "fungalits_unite"
                    },{
                        "value": "silva_138_ssu",
                        "display": "silva_138_ssu (experimental)"
                    }
                ]
            }
        },{
            "id": "minWords",
            "optional": true,
            "advanced": true,
            "allow_multiple": false,
            "field_type": "text",
            "default_values": [ null ],
            "text_options": {
                "validate_as": "int",
                "min_int": 5
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "kb_RDP_Classifier",
            "method": "run_classify",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },{
                    "narrative_system_variable": "workspace_id",
                    "target_property": "workspace_id"
                },{
                    "input_parameter": "amp_mat_upa",
                    "target_property": "amp_mat_upa",
                    "target_type_transform": "resolved-ref"
                },{
                    "input_parameter": "output_name",
                    "target_property": "output_name"
                },{
                    "input_parameter": "rdp_clsf",
                    "target_property": "rdp_clsf"
                }
            ],
            "output_mapping": [
                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },{
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
